{
    "version": 2,
    "builds": [
    {
        "src": "index.js",
        "use": "@vercel/node"
    }
    ],
    "routes": [
    {
        "src": "/(.*)",
        "dest": "index.js"
    },
    {
        "src": "/api-docs/(.*)",
        "dest": "node_modules/swagger-ui-dist/$1"
    },
    {
        "src": "/api-docs",
        "dest": "/api-docs/index.html"
    }
    ]
}